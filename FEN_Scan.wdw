   -
     internal_properties : CAAAAAgAAAA/LHQ31vD8tmoMdU3Uuk6dybfBIZyteJN/CcjG4EWn4dUProIKZv+6Q4J77N+q5tvwWVWpCw9LuATdjeEN8X+oUqKiD+D+Uj8Z4SE687iQUvflJ94/SjsST0MOkQ8H/WdquXEabwJHBGlVMRTdQo3zVShl/8RmTwVVsUh6LtPt+7tqbxYZAsXCQAnRBjkqPahISQsAozs5l4WWp0q2tjw=
     code : |1+
      
      // Test du simulateur
      SI EnModeSimulateur() ALORS
      	Erreur("Le bluetooth n'est pas disponible en mode simulateur. Veuillez exécuter cette application sur un appareil Android ou iOS.")
      	Ferme()
      FIN
      
      
      // Test de la version de l'os
      <COMPILE SI TypeConfiguration=Android>
      	SI SysVersionAndroid(sysVersionApiLevel) < 18 ALORS
      		Erreur("Votre version d'Android ne supporte pas le bluetooth 4. Vous devez disposer d'un appareil sous Android 4.3 au minimum.")
      		ferme()
      	FIN
      <sinon>
      	nVersion est un entier = ExtraitChaîne(SysVersionIOS(),1,".")
      	SI nVersion < 9 ALORS
      		Erreur("Votre version d'iOS ne supporte pas le bluetooth 4. Vous devez disposer d'un appareil sous iOS 9.0 au minimum.")
      		Ferme()
      	FIN
      <FIN>
      
      
      // Lancement du scan
      LanceScan()
