     internal_properties : CAAAAAgAAAC34ewFyg3f7YWUrdtdx9daF2eo6FvHG8cZR0XAN5GKJPy+GVyIjUgSTc60qfKBZpy6FhNvHlCqvVjzOsKEuF7/viD3vuU7XX0eeoQKj84zEm/A1lwdxQ/acNLKvvttGYQ9BXyElraP6mgcGMHK+WbL/5bCg1ZqVOU4FG5CDWZwbaPSenYWKo7BFwlWPOWGDhRxMTuehCgTvPL73wYnU+3uQoY=
     procedure_id : 1674241830394036166
     type_code : 14
     code : |1-
      // Résumé : Lance une recherche de périphérique bluetooth
      //
      PROCEDURE LanceScan()
      
      gbStopListePeriphériques = faux
      
      OPT_Arrêter..Visible = vrai
      OPT_Rafraîchir..Visible = faux
      SablierRéseau(vrai)
      
      ZoneRépétéeSupprimeTout(ZR_BTLE)
      
      // Initialisation du bluetooth
      SI PAS BLEInit() ALORS
      	ToastAffiche("Bluetooth non disponible")
      	RETOUR
      FIN
      
      TANTQUE gnEtatBluetooth <> btActif _et_ pas gbStopListePeriphériques 
      	Multitâche(-10)
      FIN
      
      // On liste les périphériques
      BLEListe()
     type : 458752
   -
     name : BLEInit
     internal_properties : CAAAAAgAAAAtMmZ05oBkBTH+fFZbmeSednNKc9z3Hgnnik+UG2WKIbda7xc42yppJiB9AcLGOfRQdXD9/bsee3K6rjzv+rsNcgOiPHaLerCS0sWxXTkhYciZyMhaCsV9Wrqd5E+RvWOM0VX6SypKINVbn3kLZ1rt1LdGHFYc/YcvGuGy0KoHyYEYZXfvGz3hWvfMBCyZh+17dxvFb6kjr52AeAh+QB7PUP6Hn5STBP95YKU5PjqPa15JXt955+PqWNsyfOd7DUgzFyjP
     procedure_id : 1674242015077673222
     type_code : 14
     code : |1-
      // Résumé : Initialise le bluetooth
      //
      PROCEDURE BLEInit()
      
      bActif est un booléen
      
      
      
      <compile si TypeConfiguration=android>
      	nEtatBTLE est un entier
      	
      	// Vérification de l'état du Bluetooth
      	nEtatBTLE = BTLEEtat()
      	
      	si nEtatBTLE <> btActif ALORS
      		//1 : &Activer
      		//2 : &Ne pas activer
      		
      		SELON Dialogue(<§?173C1A7400F4FC35§>)
      			// &Activer
      			
      			CAS 1
      				// Activation du Bluetooth
      				BTActive(Vrai)
      				bActif = vrai
      				
      				tantque BTLEEtat() <> btActif
      					Multitâche(-10)
      				FIN
      			// &Ne pas activer
      			CAS 2
      				bActif = faux
      		FIN
      	SINON 
      		bActif = vrai
      	FIN
      <sinon>
      	bActif = vrai
      <FIN>
      
      
      si bActif ALORS
      	<compile SI TypeConfiguration=Android>
      		CB_BLEInit(btActif)
      	<sinon>
      		BTLEInitialise(CB_BLEInit)
      	<FIN>
      FIN
      
      renvoyer bActif
     type : 458752
   -
     name : CB_BLEInit
     internal_properties : 
     procedure_id : 1674242663621687272
     type_code : 14
     code : |1-
      // Résumé : Fin de l'initialisation du bluetooth
      // Syntaxe :
      //CB_BLEInit (<nEtat> est entier)
      //
      // Paramètres :
      //	nEtat (entier) : Etat du bluetooth
      //
      PROCEDURE CB_BLEInit(nEtat est un entier)
      
      gnEtatBluetooth = nEtat
      
      si gnEtatBluetooth <> btActif ALORS
      	Erreur("Le Bluetooth n'est pas activé sur votre appareil.","Veuillez l'activer afin d'utiliser cet exemple.")
      FIN
