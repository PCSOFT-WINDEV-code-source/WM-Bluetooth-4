     type : 458752
   -
     name : CB_BLEListe
     internal_properties : CAAAAAgAAAD1lrTZd97I6+eSvmqZHq+BRyXrQV8+1kzp6bsEclzT0u3bYSw5nx3TMv/ZUM52Q2wkKrkNHZbbPxFIjNGjqWlD7JYbHD5eketkzFMb0GFlp0qHG4EIzOdM7FIr3qjFn8HUB1+AvajAnkdtwR+Z6WSb4t2k0szmzyGF3AtWctBvkcuqh0X/Gz2h2tcu5uqjCXP30/uHCSe1ASNu/K7eqEDtRsjNzV5Z0MtVdLE9fjppdQWbWG8=
     procedure_id : 1674243106003552207
     type_code : 14
     code : |1+
      // Résumé : Callback appelée lorsqu'un périphérique est découvert
      // Syntaxe :
      //[ <Résultat> = ] CB_BLEListe (<MonPériph> est BTLEPériphérique)
      //
      // Paramètres :
      //	MonPériph (BTLEPériphérique) :Le périphérique trouvé
      
      PROCEDURE CB_BLEListe(MonPériph est un BTLEPériphérique)
      
      bExiste est un booléen
      
      si gbStopListePeriphériques ALORS
      	renvoyer faux
      FIN
      
      // On vérifie si on n'a pas déjà ajouté ce périphérique
      pour TOUT tPeriph de gtabPeriphériques 
      	si tPeriph.Identifiant = MonPériph.Identifiant ALORS
      		bExiste = vrai
      	FIN
      FIN
      
      si pas bExiste ALORS
      	// Ajout du périphérique en mémoire
      	TableauAjoute(gtabPeriphériques,MonPériph)
      	
      	// Ajout du périphérique dans la Zone répétée
      	sNom est une chaîne = MonPériph.Nom
      	si sNom ~= "" ALORS
      		sNom = "Périphérique sans nom"
      	FIN
      	ZoneRépétéeAjouteLigne(ZR_BTLE,MonPériph.Identifiant,sNom)
      FIN
      
      renvoyer vrai
